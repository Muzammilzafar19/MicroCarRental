<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="design.ws.com.MicroCarRental">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/micrologo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/micrologo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ToursVahicalActivity"></activity>
        <activity android:name=".ThirdPartyEmailActivity" />
        <activity android:name=".Activity_Name_List" />
        <activity android:name=".Category_Type_Activity" />
        <activity android:name=".Filter_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Booking_Table_Activity" />
        <activity
            android:name=".BookNowActivity"
            android:label="Book Now"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="design.ws.com.MicroCarRental.MainActivity" />
        </activity>
        <activity
            android:name=".ContactUsActivity"
            android:label="Contact Us"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="design.ws.com.MicroCarRental.MainActivity" />
        </activity>
        <activity
            android:name=".AboutUsActivity"
            android:label="About Us"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="design.ws.com.MicroCarRental.MainActivity" />
        </activity>
        <activity
            android:name=".CarForWeddingActivity"
            android:label="Car for Wedding"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="design.ws.com.MicroCarRental.MainActivity" />
        </activity>
        <activity
            android:name=".VahicalForTourActivity"
            android:label="Vehicle for Tour"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ExoticCarActivity"
            android:label="Exotic and Luxury Cars"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="design.ws.com.MicroCarRental.MainActivity" />
        </activity>
    </application>

</manifest>